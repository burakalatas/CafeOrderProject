@model IEnumerable<StaffInfo>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_StaffLayout.cshtml";
}

<h2 style="text-align:center; margin-top:0.5em; margin-bottom:0.5em; color:#FAAB78;">Staff</h2>

<table class="table table-bordered">
    <tr>
        <th>Staff ID</th>
        <th>Staff Name</th>
        <th>Staff Phone No</th>
        <th>Staff Type</th>
    </tr>
    @foreach (var item in Model)
    {
            <tr>
                <td>@item.id</td>
                <td>@item.name</td>
                <td>@item.phoneNo</td>
                <td>@item.staffType</td>
            </tr>
    }
</table>


